  		// 添加宝贝监控
  		add_mointor_item: function(){
  			var opt = {
  				server: 'rc',
  				url: SERVER_Path['add_mointor_item'],
  				type: 'post',
  				params: {
  					'api_name': 'add_mointor_item'
  				}
  			};
  			$.extend(opt.params, arguments[0]);
  			arguments[0] = opt;
  
  			// 添加宝贝监控 ga 跟踪
  			var val = '用户' + RC.user.get('id') + '_' + opt.params.title
  				, success = arguments[1]
  				, _success = function (json) {
  					success && success(json);
  				}
  
  			arguments[1] = _success;
  
  
  			return doserverCall.apply(RC, arguments);
  		},
  
