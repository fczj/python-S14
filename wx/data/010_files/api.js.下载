  		get_user_levelinfo:function(user_info, success, fail){
  			var opt = {
  					server: 'rc',
  					url: SERVER_Path['get_user_levelinfo'],
  					type: 'get',
  					params: {
  						'api_name': 'get_user_levelinfo'
  					}
  				};
  			var __success= function(json){
  				if(json.result==1){
  					user_info.data.level = json.data.level;
  					user_info.data.isPrimaryLevel = json.data.ifPrimaryLevel;
  				    user_info.data.primaryLevelStartTime = json.data.primaryLevelStartTime;
  				    user_info.data.primaryLevelEndTime = json.data.primaryLevelEndTime;
  				    user_info.data.isSeniorLevel = json.data.ifSeniorLevel;
  				    user_info.data.seniorLevelStartTime = json.data.seniorLevelStartTime;
  				    user_info.data.seniorLevelEndTime = json.data.seniorLevelEndTime;
  				    user_info.data.isLuxuryLevel = json.data.ifLuxuryLevel;
  				    user_info.data.luxuryLevelStartTime = json.data.luxuryLevelStartTime;
  				    user_info.data.luxuryLevelEndTime = json.data.luxuryLevelEndTime;
  
  					if(user_info.data.level === 'luxury'){
  						user_info.data.__level = 'luxury';
  						user_info.data.level = 'senior';
  					}
  					// if(user_info.data.mobile == '18906538803'){
  					// 	user_info.data.__level = 'luxury';
  					// }
  				}
  				success&&success(user_info);
  			}
  			$.extend(opt.params, {});
  			return doserverCall(opt, __success, fail, false, false, false);
  		},
  		get_user_info: function(params, success, fail){
  			var opt = {
  					server: 'rc',
  					url: SERVER_Path['get_user_info'],
  					type: 'get',
  					params: {
  						'api_name': 'get_user_info'
  					}
  				};
  			$.extend(opt.params, {});
  			var __success= function(json){
  				if(json.result==1){
  					API.get_user_levelinfo(json,success,fail);
  				}else{
  					success&&success(json);
  				}
  			}
  			// 不出现提示框信息
  			return doserverCall(opt, __success, fail, false, false, false);
  		},
