  		user_change_pay: function(){
  			var opt = {
  				server: 'rc',
  				url: SERVER_Path['user_change_pay'],
  				type: 'POST',
  				params: {
  					'api_name': 'user_change_pay'
  				}
  			};
  
  			$.extend(opt.params, arguments[0]);
  			arguments[0] = opt;
  
  			// isPreview 为真时, 表示套餐价格查询
  			if (!opt.isPreview) {
  				// 添加套餐购买 ga 跟踪
  				var success = arguments[1]
  					, tp = {
  						'10001': '套餐,标准套餐'
  						, '10002': '套餐,高级套餐'
  						, '1': '短信,试用型'
  						, '2': '短信,标准型'
  						, '3': '短信,尊贵型'
  						, '4': '短信,企业型'
  						, '5': '邮件,试用型'
  						, '6': '邮件,标准型'
  						, '7': '邮件,尊贵型'
  						, '8': '邮件,企业型'
  					}[opt.params.changeRuleId].split(',')
  					, val = '用户' + RC.user.get('mobile') + '_' + tp[1];
  
  				var _success = function (json) {
  					success && success(json);
  				};
  
  				arguments[1] = _success;
  			}
  
  			return doserverCall.apply(RC, arguments);
  		},
