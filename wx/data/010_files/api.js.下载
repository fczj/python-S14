define('framework/js/api', function(require, exports, module) {

  /**
   * @author xukang
   */
  ;(function($, RC) {
  
  	// var filter = require("www:filter/filter-config.js");
  	/**
  	 * API 层是与server层的调用, 页面只能够访问API而不能够直接越过API层对后面的数据调用.
  	 */
  	var doserverCall = function(opt, success, fail, show1, show2, show3){
  		var after =  RC.serverCall;
  		// filter({option:opt,success:success,fail:fail,call_show1:show1,call_show2:show2,call_show3:show3},after);
  		RC.serverCall(opt, success, fail, show1, show2, show3)
  	}
  
  var doserverCall_2 = function(opt, success, fail, show1, show2, show3){
  		var after =  RC.serverCall;
  		filter({option:opt,success:success,fail:fail,call_show1:show1,call_show2:show2,call_show3:show3},after);
  		// RC.serverCall(opt, success, fail, show1, show2, show3)
  	}
  
  
  
  	var API = {
  		get_user_identifycode: function(params, success, fail){
  			var opt = {
  					server: 'rc',
  					url: SERVER_Path['get_user_identifycode'],
  					type: 'get',
  					params: {
  						'api_name': 'get_user_identifycode'
  					}
  				};
  			$.extend(opt.params, params);
  			return doserverCall(opt, success, fail, false, false, false);
  		},
  		get_user_email_identifycode: function(params, success, fail){
  			var opt = {
  					server: 'rc',
  					url: SERVER_Path['get_user_email_identifycode'],
  					type: 'get',
  					params: {
  						'api_name': 'get_user_email_identifycode'
  					}
  				};
  			$.extend(opt.params, params);
  			return doserverCall(opt, success, fail, false, false, false);
  		},
  		post_user_register: function(params, success, fail){
  			var opt = {
  					server: 'rc',
  					url: SERVER_Path['post_user_register'],
  					type: 'POST',
  					params: {
  						'api_name': 'post_user_register'
  					}
  				};
  
  			// 添加注册 ga 跟踪
  			var _success = function (json) {
  				success && success(json);
  			};
  
  			$.extend(opt.params, params);
  			return doserverCall(opt, _success, fail, false, false, false);
  		},
  		get_user_validatecode: function(params, success, fail){
