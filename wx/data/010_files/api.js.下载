  				};
  
  			// 添加登录 ga 登录
  			var _success = function (json) {
  				success && success(json);
  			};
  
  			$.extend(opt.params, params);
  			return doserverCall(opt, _success, fail, is_need_loading, is_need_success, is_need_fail);
  		},
