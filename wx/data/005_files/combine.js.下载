;
;(function ($, undefined) {
var iSpyscroll = function ($ele) {
	var _this = this;
	_this.spy($ele);
}

iSpyscroll.prototype = {
	constructor: iSpyscroll
	, spy: function ($ele) {
		var _this = this;
		_this.$box = $($ele);
		_this.$anchors = _this.$box.find('.anchor');
		
		var target_arr = [];
		$.each(_this.$anchors, function (i) {
			target_arr.push($(_this.$anchors.eq(i).attr('data-target'))[0]);
		});
		_this.$targets = $(target_arr);
		
		_this.$placeholder = $('<div>');
		_this.$placeholder.css({
			position: 'static'
			, display: 'none'
			, width: _this.$box.outerWidth()
			, height: _this.$box.outerHeight()
		});
		_this.$box.after(_this.$placeholder);
		
		_this.bind();
		_this._spy();
	}
	
	, _spy: function () {
		var _this = this
			, rect1 = _this.$box[0].getBoundingClientRect()
			, rect2 = _this.$placeholder[0].getBoundingClientRect()
			, height = rect1.bottom - rect1.top;
			
		if (rect1.top < 0) {
			_this.fixed();
		}
		else if (rect2.top > 0) {
			_this.static();
		}
		
		// 渲染锚点
		if (!_this._gc_goto_status) {
			$.each(_this.$targets, function (i) {
				var $target = _this.$targets.eq(i)
					, rect = $target[0].getBoundingClientRect();
				if (
					// 向下滚动时
					(0 < rect.top && rect.top < height)
					
					// 向上滚动时
					|| (rect.top < 0 && (0 < rect.bottom && rect.bottom > height + 30))) {
					
					_this.$anchors.removeClass('current')
						.eq(i).addClass('current');
					//return false;
				}
			});
		}
	}
	
	, fixed: function () {
		var _this = this;
		_this.$box.addClass('fixed');
		_this.$placeholder.show();
	}
	
	, static: function () {
		var _this = this;
		_this.$box.removeClass('fixed');
		_this.$placeholder.hide();
	}
	
	, bind: function () {
		var _this = this;
		
		if (_this._gc_binded_status) {
			return;
		}
		
		var i = 0
			, $win = $(window)
			, box_height = _this.$box.outerHeight();
		
		_this.$anchors.on('click.ctrl_pin', function () {
			var idx = _this.$anchors.index(this)
				, rect = _this.$targets.get(idx).getBoundingClientRect()
				, y = $win.scrollTop() + rect.top - box_height - 10;
				
			_this.$anchors.removeClass('current')
				.eq(idx)
					.addClass('current');
			
			_this._gc_goto_status = true;
			
			$('html, body').animate({
				scrollTop: y
			}, {
				duration: 300
				, complete: function () {
					_this._gc_goto_status = false;
				}
			});
		});
		
		$win.on('scroll.ctrl_pin', function () {
			if ($(window).scrollTop() == 0) {
				_this._spy();
			}
		
			if (i++ % 2) {
				return;
			}
			
			_this._spy();
		});
		
		_this._gc_binded_status = true;
	}
	
	, unbind: function () {
		var _this = this;
		_this.$anchors.off('click.ctrl_pin');
		$(window).off('scroll.ctrl_pin');
	}
	
	, destroy: function () {
		var _this = this;
		_this.unbind();
		_this._gc_binded_status = null;
	}

}

;(function () {
	var old = $.fn.iSpyscroll;
	$.fn.iSpyscroll = function (config) {
		return this.each(function () {
			var $this = $(this)
				, data = $this.data('iSpyscroll')
				, opts = $.extend({}, config);
			
			if (config == 'destroy') {
				data && data.destroy();
				return;
			}
			
			if (!data) {
				$this.data('iSpyscroll', (data = new iSpyscroll($this)));
			}
		});
	}
	$.fn.iSpyscroll.constructor = iSpyscroll;
	$.fn.iSpyscroll.noConflict = function () {
		$.fn.iSpyscroll = old;
		return this;
	}
})();


}(jQuery));;
;(function ($, undefined) {
	// 默认规则
	var SYS_RULE = {
		required: function (val) {
			if (typeof val == 'boolean') {
				return val;
			}
			
			return !this.empty(val);
		}
		
		, empty: function (val) {
			return !$.trim(val);
		}
		
		, chars: function (val) {
			return !/[^\w]/.test(val);
		}
		
		, number: function (val) {
			return this.decimal(val);
		}
		
		, integer: function (val) {
			return /^(?:-?[1-9]\d*|0)$/.test(val);
		}
		
		, decimal: function (val) {
			return /^-?(?:(?:0?(\.\d*)*)|[1-9]\d*\.?\d*)$/.test(val);
		}
		
		, minlength: function (val, limit) {
			if (!this.integer(limit)) {
				return true;
			}
			
			// minlength 理论上最小值为 1
			// 所以, val 不存在时, 应该返回 false
			return (val ? val.length >= limit : false);
		}
		
		, maxlength: function (val, limit) {
			if (!this.integer(limit)) {
				return true;
			}
			
			return (val ? val.length <= limit : false);
		}
		
		, eq: function (val, limit) {
			return val === limit;
		}
		
		, min: function (val, limit) {
			if (Number(limit)) {
				val = val * 1;
				limit = limit * 1;
			}
			
			return val >= limit;
		}
		
		, max: function (val, limit) {
			if (Number(limit)) {
				val = val * 1;
				limit = limit * 1;
			}
			
			return val <= limit;
		}
		
		, mobile: function (val) {
			/* 号码段来自 http://t.cn/Rv6QvRr */
            return /^(13[0-9]|14[57]|15[0-35-9]|17[6-8]|18[0-9])\d{8}$|170[059]\d{7}$/.test(val);
		}
		
		, email: function (val) {
			return /^([a-z\d]+[\._]?)*[a-z\d]+@([a-z\d]+[\._]?)*[a-z\d]+\.[a-z]{2,4}$/i.test(val);
		}
		
		, zip: function () {}
		
		, chinese: function () {}
		
		, english: function () {}
	};
	

	var iValidator = function ($ele, opts) {
		var _this = this;
		_this.setOptions(opts);
		_this.init($ele);
	}
	
	iValidator.prototype = {
		constructor: iValidator
		
		, init: function ($ele) {
			var _this = this;
			
			_this.$box = $($ele);
			_this.RULE_MAP = $.extend(true, {}, SYS_RULE);
			_this.VALIDATE_RESULT = {};
		}
		
		, setOptions: function (opts) {
			var _this = this
				, defaults = {
					errTpl: ''
					, errCls: 'ipt-error'
					, autoTip: true
					, validateType: 'single'
				};
			_this.opts = $.extend(defaults, opts);
		}
		
		/**
		 * @description 添加验证规则
		 * @param name {String} 规则名
		 * @param rule {RegExp | Function} 验证规则
		 */
		, addRule: function (name, rule) {
			var _this = this;
			
			if (typeof rule != 'function') {
				rule = function (val) {
					return rule.test(val);
				}
			}
			
			_this.RULE_MAP[name] = rule;
		}
		
		, validate: function () {
			var _this = this;
			_this.VALIDATE_RESULT = {};
			
			_this.$box.find(_this.opts.errCls)
				.removeClass(_this.opts.errCls);
			
			$.each(_this.$box.find('[data-rules]'), function (i, ele) {
				if (_this.opts.validateType == 'single' && !$.isEmptyObject(_this.VALIDATE_RESULT)) {
					return false;
				}
				
				_this._validate($(ele));
			});
		}
		
		, addErrMsg: function (ele, msg) {
			var _this = this
				, $ele = $(ele)
				, $err = _this.$box.find($ele.attr('data-err-target'));
			
			$ele.addClass(_this.opts.errCls);
			$err.show().html(msg);
		}
		
		, removeErrMsg: function (ele) {
			var _this = this
				, $ele = $(ele)
				, $err = _this.$box.find($ele.attr('data-err-target'));
			
			$ele.removeClass(_this.opts.errCls);
			$err.hide().html('');
		}
		
		, _validate: function (ele) {
			var _this = this
				, $ele = $(ele);
						
			if (!$ele.length
				|| !/input|textarea/i.test($ele[0].tagName)) {
				return;
			}
			
			var	$rely = $($ele.attr('data-rely'))
				, rules = $ele.attr('data-rules').split('&&')
				, err_tips = $ele.attr('data-errs').split('##')
				, val = $ele.val();
			
			
			if (/^(radio|checkbox)$/i.test($ele.attr('type'))) {
				// 单选、复选框取 checked 值
				val = $ele.prop('checked');
			}
			
			// 存在依赖, 但依赖没有值时, 退出
			if ($rely.length) {
				if (/^(radio|checkbox)$/i.test($rely.attr('type'))) {
					if (!$rely.filter(':checked').length) {
						return;
					}
				}
				else if (!$.trim($rely.val())) {
					return;
				}
			}
			
			var err_txt;
			if ($.inArray('required', rules) != -1 || val) {
				$.each(rules, function (i, rule) {
					// @todo 有的 rule 还是 email||mobile
					
					// 有的 rule 可能存在 = 号
					// 比如, min=xxx
					var _rule = rule.split('=')
						, limit = _rule[1];
					
					rule = _rule[0];
					
					if (limit && isNaN(Number(limit))) {
						limit = _this.$box.find(limit).val() || '';
					}
					
					if (!_this.RULE_MAP[rule](val, limit)) {
						err_txt = err_tips[i];
						return false;
					}
				});
			}
			
			var name = $ele.attr('name');
				
			if (err_txt) {
				_this.VALIDATE_RESULT[name] = err_txt;
				_this.addErrMsg($ele, err_txt);
			}
			else {
				delete _this.VALIDATE_RESULT[name]
				_this.removeErrMsg($ele);
			}
		}
		
	}
	
	
	;(function () {
		var old = $.fn.iValidator;
		$.fn.iValidator = function (config) {
			return this.each(function () {
				var $this = $(this)
					, data = $this.data('iValidator')
					, opts = $.extend({}, config || {});
				
				if (!data) {
					$this.data('iValidator', (data = new iValidator($this, opts)));
				}
				
				data.validate();
			});
		}
		$.fn.iValidator.constructor = iValidator;
		$.fn.iValidator.noConflict = function () {
			$.fn.iValidator = old;
			return this;
		}
	}());

}(jQuery));

/*
<input type="text" data-rules="required&&mobile||email" data-errs="" data-reply=".xxx" data-err-target=".xx" />
var rlt = $().iValidator();


*/










;
;(function ($, undefined) {
	
	// 为 $.dialog 添加特殊弹框
	$.dialog.showSuccess = function (msg, timeout, callback) {
		msg = msg || '恭喜您, 操作成功!';
		timeout = timeout || 2;
		callback = callback || function () {};
		$.dialog({
			title: false
			, cancel: false
			, fixed: true
			, lock: true
			, resize: false
			, close: callback
		})
		.content(msg)
		.time(timeout, function () {
			this.DOM.icon[0].innerHTML = '<img src="http://static.superboss.cc/newweb/js/dev/global/skins/icons/ok.png" class="ui_icon_bg"/>';
			this.DOM.icon[0].style.display = '';
		});
	}
	
	
	$.dialog.showWarn = function(msg, timeout, callback) {
		msg = msg || '操作失败, 请重新尝试!';
		timeout = timeout || 2;
		callback = callback || function () {};
		$.dialog({
			title: false
			, cancel: false
			, fixed: true
			, lock: true
			, resize: false
			, close: callback
		})
		.content(msg)
		.time(timeout, function () {
			this.DOM.icon[0].innerHTML = '<img src="http://static.superboss.cc/newweb/js/dev/global/skins/icons/warn.png" class="ui_icon_bg"/>';
			this.DOM.icon[0].style.display = '';
		});
	};
	
	
	
	// 隐藏页面中含有 .J_close-parent 的父元素
	$('document').ready(function () {
		$('.J_close-parent').on('click', function () {
			var parent = $(this).parent();
			parent.animate({
				opacity: 0
			}, function () {
				parent.css('display', 'none');
			});
	
			return false;
		});
	});
	
	$(document).ready(function () {
	
		function chunk(items, process, context){
			var todo = items.concat();
			var f = function () {
				var start = +new Date();
				do {
					 process.call(context, todo.shift());
				} while (todo.length > 0 && (+new Date() - start < 500));
		
				if (todo.length > 0){
					setTimeout(arguments.callee, 25);
				}
			};
			f();
		}
	
	
		/* 初始化日期拾取 */
		if ($.fn.datepicker) {
			$('body').on('click', function (e) {
				var $target = $(e.target)
					, data = $target.data('datepicker');
				if (data) {
					data.show();
					return;
				}
				
				if ($target.hasClass('date-picker')) {
					$target.datepicker().show();
					$target.data('datepicker').show();
				}
			});
		}
	
	
		/* 初始化 .dropdown 下拉弹框 */
		$('body').on('click', function (e) {
			var $target = $(e.target);
			if ($target.hasClass('dropdown-trigger-txt')
				|| $target.hasClass('dropdown-trigger-arrow')
				|| $target.parent().hasClass('dropdown-trigger-arrow')) {
	
				$dropdown = $target.parents('.dropdown');
				$dropdown.toggleClass('open');
				$('.dropdown').each(function (i, o) {
					if ($.contains(o, $target[0])) {
						return;
					}
					$(o).removeClass('open');
				});
				last = $dropdown;
				$('.close-dropdown', $dropdown).on('click', function () {
					$dropdown.removeClass('open');
				});
	
				var $dropdownMenu = $('.dropdown-menu', $dropdown);
				if (!$dropdownMenu.data('dropdownMenuClick')) {
					$dropdownMenu.on('click', function () {
						return false;
					});
					$dropdownMenu.data('dropdownMenuClick', true);
				}
			}
			else {
				$('.dropdown').removeClass('open');
			}
		});
		
	
		/* 初始化 <select> */
		if ($.fn.iSelect) {
			//$('select').iSelect();
		}
		
		
		if ($.fn.iTab) {
			//$('.ui-tab').iTab();
		}
	
	
		/* 初始化 tooltip 提示 */
		if ($.fn.iTip) {
			$('.tool-tip').iTip({
				skin: 'orange'
				, triggerType: 'hover'
			});
		}
	
	});
	
})(jQuery);
