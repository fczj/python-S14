      RC.template.addTemp('template.shop.detail.item_analysis.onsale', tempOnsale);
        RC.template.addTemp('template.shop.detail.item_analysis.onnew', tempOnnew);
      RC.template.addTemp('template.shop.detail.item_analysis.onname', tempOnname);
      RC.template.addTemp('template.shop.detail.item_analysis.onprice', tempOnprice);
      //RC.template.addTemp('template.shop.detail.item_analysis.traded', tempTraded);
  })(jQuery,RayCloud);
  

});
